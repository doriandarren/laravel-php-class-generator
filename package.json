{
  "name": "laravel-php-class-generator",
  "displayName": "Laravel PHP Class Generator",
  "description": "Generates PHP classes with a folder-based namespace.",
  "version": "0.0.1",
  "publisher": "infinito",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:laravelPhpClassGenerator.createPhpClass"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "laravelPhpClassGenerator.createPhpClass",
        "title": "Crear clase PHP (Laravel)",
        "category": "Laravel PHP"
      }
    ],
    "keybindings": [
      {
        "command": "laravelPhpClassGenerator.createPhpClass",
        "key": "cmd+enter",
        "when": "editorTextFocus && editorLangId == php",
        "mac": "cmd+enter"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/doriandarren/laravel-php-class-generator"
  }
}
